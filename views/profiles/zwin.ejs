                          <h3 class="main-heading"><span class="fa fa-github"></span> GitHub Repositories Lanhuages</h3>
                          <ul class="tag-cloud">
                              <% _.each(github_repos_tag_cloud.cloud,function(count,tag){ %>
                                <%
                                var size = 2 * ((count - github_repos_tag_cloud.min) / (github_repos_tag_cloud.max - github_repos_tag_cloud.min)) + 1.2
                                %>
                                <li><span style="font-size:<%=size%>em"><%=tag%></span></li>
                              <% }) %>
                          </ul>
