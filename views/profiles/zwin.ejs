    <body>
        <!-- Container -->
        <section id="container">

            <!-- Header -->
			<header>
            	<!-- Logo -->
            	<div id="logo">
                	<h2>ANDERSON SMITH</h2>
                    <h4>GRAPHIC WEB DESIGNER</h4>
                </div>
                <!-- /Logo -->

                <!-- Social Icons -->
                <ul class="socialicons">
                    <li class="socials-text">Professional Profiles</li>
                    <% if(typeof linkedin_profile_link != 'undefined'){ %>
                      <li><a href="<%=linkedin_profile_link%>" class="transition"><span class="fa fa-linkedin"></span></a></li>
                    <% } %>
                    <% if(typeof github_profile_link != 'undefined'){ %>
                      <li><a href="<%=github_profile_link%>" class="transition"><span class="fa fa-github"></span></a></li>
                    <% } %>
                    <% if(typeof stackoverflow_profile_link != 'undefined'){ %>
                      <li><a href="<%=stackoverflow_profile_link%>" class="transition"><span class="fa fa-stack-overflow"></span></a></li>
                    <% } %>
                    <% if(typeof meetup_profile_link != 'undefined'){ %>
                      <li><a href="<%=meetup_profile_link%>" class="transition"><span class="fa fa-meetup"></span></a></li>
                    <% } %>
                </ul>
                <!-- /Social Icons -->
            </header>
            <!-- /Header -->

            <!-- Content -->
            <section id="content" >

                <!-- Profile -->
                <div id="profile">
                 	<!-- About section -->
                	<div class="about">
                    	<div class="photo-inner">
                            <ul>
                                <li><img src="<%=(typeof linkedin_summary != 'undefined' ? linkedin_summary.pictureUrl : '')%>" height="186" width="153" /></li>
                                <!-- <li><img src="images/photo1.png" height="186" width="153" /></li> -->
                            </ul>
                        </div>
                        <h1><%=(typeof linkedin_summary != 'undefined' ? linkedin_summary.firstName + ' ' + linkedin_summary.lastName : '') %></h1>
                        <h3><%=(typeof linkedin_summary != 'undefined' ? linkedin_summary.headline: '')%></h3>
                        <p><%=(typeof linkedin_summary != 'undefined' ? linkedin_summary.summary: '')%></p>
                    </div>
                    <!-- /About section -->

                    <!-- Personal info section -->
                	<ul class="personal-info">
                      <li><label>Location</label><span><%=(typeof linkedin_summary != 'undefined' ? linkedin_summary.location.name: '')%></span></li>
                      <li><label>Industry</label><span><%=(typeof linkedin_summary != 'undefined' ? linkedin_summary.industry: '')%></span></li>
                      <li><label>Connections</label><span><%=(typeof linkedin_summary != 'undefined' ? linkedin_summary.numConnections: '')%><%=(typeof linkedin_summary != 'undefined' ? (linkedin_summary.numConnectionsCapped ? '+' : ''): '')%></span></li>
                  </ul>
                    <!-- /Personal info section -->
                </div>
                <!-- /Profile -->

