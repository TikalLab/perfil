                                <div class="value"><%=stackoverflow_basic_stats.badges.silver%></div>
                              </li>
                              <li>
                                <h4>Bronze Badges</h4>
                                <div class="value"><%=stackoverflow_basic_stats.badges.bronze%></div>
                              </li>
                          </ul>
                        <% } %>


                        <% if(typeof meetup_group_categories_tag_cloud != 'undefined'){ %>
                          <h3 class="main-heading"><span class="fa fa-meetup"></span> Meetup Groups Categories</h3>
